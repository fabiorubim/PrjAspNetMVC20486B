@{
    ViewBag.Title = "Index";
}

<h2>Demo JQueryUI</h2>

<link href="~/Content/themes/base/all.css" rel="stylesheet" />

<div class="ui-widget">
  <label for="tags">Tags: </label>
  <input id="tags">
</div>

@section scripts{
    <script src="~/Scripts/jquery-ui-1.12.1.js"></script>

    <script>
        $(function () {
            $.get("@Url.Action("GetData")").success(function (dados) { 
                $("#tags").autocomplete({
                    source: dados            
              });            
            });
        });
  </script>
}
